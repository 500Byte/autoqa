{
  "name": "autoqa",
  "version": "0.1.0",
  "private": true,
  "main": "electron.cjs",
  "scripts": {
    "dev": "next dev",
    "build": "next build --webpack",
    "start": "next start",
    "lint": "eslint",
    "preview:electron": "cross-env NODE_ENV=production next build --webpack && electron .",
    "electron:dev": "cross-env NODE_ENV=development concurrently \"npm run dev\" \"wait-on http://localhost:3000 && electron .\"",
    "electron:build": "cross-env NODE_ENV=production npm run build && electron-builder",
    "electron:start": "electron ."
  },
  "dependencies": {
